{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecuteMsg",
  "oneOf": [
    {
      "description": "Deposit OSMO to earned locked MBRN rewards for a specified duration",
      "type": "object",
      "required": [
        "lock"
      ],
      "properties": {
        "lock": {
          "type": "object",
          "required": [
            "lock_up_duration"
          ],
          "properties": {
            "lock_up_duration": {
              "description": "Lock duration of MBRN rewards, in days",
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Change lockup duration of a subset of locked deposits.",
      "type": "object",
      "required": [
        "change_lock_duration"
      ],
      "properties": {
        "change_lock_duration": {
          "type": "object",
          "required": [
            "new_lock_up_duration",
            "old_lock_up_duration"
          ],
          "properties": {
            "new_lock_up_duration": {
              "description": "Lock duration of MBRN rewards, in days",
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "old_lock_up_duration": {
              "description": "Lock duration of MBRN rewards, in days",
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "uosmo_amount": {
              "description": "Amount of uosmo to change lock duration of",
              "anyOf": [
                {
                  "$ref": "#/definitions/Uint128"
                },
                {
                  "type": "null"
                }
              ]
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Withdraw OSMO from a specified lockup duration",
      "type": "object",
      "required": [
        "withdraw"
      ],
      "properties": {
        "withdraw": {
          "type": "object",
          "required": [
            "lock_up_duration",
            "withdrawal_amount"
          ],
          "properties": {
            "lock_up_duration": {
              "description": "Lock duration of MBRN rewards, in days",
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "withdrawal_amount": {
              "description": "OSMO amount to withdraw",
              "allOf": [
                {
                  "$ref": "#/definitions/Uint128"
                }
              ]
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Claim MBRN rewards from a specified lockup duration. Must be past the lockup duration to claim rewards.",
      "type": "object",
      "required": [
        "claim"
      ],
      "properties": {
        "claim": {
          "type": "object",
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Create MBRN & CDT LPs. Incentivize CDT stableswap. Deposit into MBRN OSMO LP.",
      "type": "object",
      "required": [
        "launch"
      ],
      "properties": {
        "launch": {
          "type": "object",
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Update Config",
      "type": "object",
      "required": [
        "update_config"
      ],
      "properties": {
        "update_config": {
          "$ref": "#/definitions/UpdateConfig"
        }
      },
      "additionalProperties": false
    }
  ],
  "definitions": {
    "Uint128": {
      "description": "A thin wrapper around u128 that is using strings for JSON encoding/decoding, such that the full u128 range can be used for clients that convert JSON numbers to floats, like JavaScript and jq.\n\n# Examples\n\nUse `from` to create instances of this and `u128` to get the value out:\n\n``` # use cosmwasm_std::Uint128; let a = Uint128::from(123u128); assert_eq!(a.u128(), 123);\n\nlet b = Uint128::from(42u64); assert_eq!(b.u128(), 42);\n\nlet c = Uint128::from(70u32); assert_eq!(c.u128(), 70); ```",
      "type": "string"
    },
    "UpdateConfig": {
      "type": "object",
      "properties": {
        "credit_denom": {
          "description": "Basket credit asset denom",
          "type": [
            "string",
            "null"
          ]
        },
        "mbrn_denom": {
          "description": "MBRN token denom",
          "type": [
            "string",
            "null"
          ]
        },
        "osmo_denom": {
          "description": "OSMO denom",
          "type": [
            "string",
            "null"
          ]
        },
        "usdc_denom": {
          "description": "Axelar USDC denom",
          "type": [
            "string",
            "null"
          ]
        }
      },
      "additionalProperties": false
    }
  }
}
